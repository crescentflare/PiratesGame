{
  "modules": [
    {
      "module": "alert"
    },
    {
      "module": "navigation"
    }
  ],
  "dataSets": {
    "layout": {
      "viewlet": "navigationContainer",
      "width": "stretchToParent",
      "height": "stretchToParent",
      "recycling": true,
      "topBar": {
        "viewlet": "simpleNavigationBar",
        "backgroundColor": "$primary",
        "localizedTitle": "GLOBAL_APP_NAME",
        "menuActionText": "Quit",
        "menuActionEvent": "navigate://back"
      },
      "bottomBar": {
        "viewlet": "bottomNavigationBar",
        "backgroundColor": "$primary"
      },
      "content": {
        "viewlet": "level",
        "width": "stretchToParent",
        "height": "stretchToParent",
        "backgroundColor": "#63a6c7",
        "tileMap": [
          " #  #",
          "   E ",
          "# ###",
          "    #",
          "# # #",
          " P   ",
          "## ##",
          " # # "
        ],
        "waveSpawnInterval": 0.2
      }
    }
  }
}
